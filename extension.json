{
    "manifest_version": 2,
    "name": "JWTAuth",
    "version": "0.1.3",
    "author": [
            "[https://github.com/mywikis MyWikis LLC]"
    ],
    "url": "https://github.com/jeffw16/JWT",
    "descriptionmsg": "jwt-ext-desc",
    "license-name": "MIT",
    "type": "other",
    "requires": {
        "MediaWiki": ">= 1.35.0"
    },
    "MessagesDirs": {
        "JWT": [
            "i18n"
        ]
    },
    "AutoloadNamespaces": {
        "MediaWiki\\Extension\\JWTAuth\\": "includes/"
    },
    "SpecialPages": {
        "JWTLogin": {
            "class": "MediaWiki\\Extension\\JWTAuth\\JWTLogin"
        }
    },
    "Hooks": {
        "TitleReadWhitelist": "readcheck"
    },
    "HookHandlers": {
        "readcheck": {
		"class": "MediaWiki\\Extension\\JWTAuth\\Hooks",
		"services": [ "SpecialPageFactory" ]
	}
    },
    "ExtensionMessagesFiles": {
        "JWTAuthAlias": "JWTAuth.alias.php"
    },
    "ConfigRegistry": {
        "JWTAuth": "GlobalVarConfig::newInstance"
    },
    "config": {
        "JWTAuthAlgorithm": {
            "value": "HS256"
        },
        "JWTAuthKey": {
            "value": ""
        },
        "JWTRequiredClaims": {
            "value": []
        },
        "JWTGroupMapping": {
            "value": []
        },
        "JWTAuthDebugMode": {
            "value": false
        },
        "JWTGroupsClaimName": {
            "value": "groups"
        }
    },
    "load_composer_autoloader": true
}
