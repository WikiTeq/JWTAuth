{
    "manifest_version": 2,
    "name": "JWTAuth",
    "version": "0.1.0",
    "author": [
            "[https://github.com/mywikis MyWikis LLC]"
    ],
    "url": "https://github.com/jeffw16/JWT",
    "descriptionmsg": "jwt-ext-desc",
    "license-name": "MIT",
    "type": "other",
    "requires": {
        "MediaWiki": ">= 1.35.0"
    },
    "MessagesDirs": {
        "JWT": [
            "i18n"
        ]
    },
    "AutoloadNamespaces": {
        "MediaWiki\\Extension\\JWTAuth\\": "includes/"
    },
    "config": {
        "JWTAuthAlgorithm": {
            "value": "HS256"
        },
        "JWTAuthKey": {
            "value": ""
        },
        "JWTRequiredClaims": {
            "value": []
        },
        "JWTGroupMapping": {
            "value": []
        },
        "JWTGroupsClaimName": {
            "value": "groups"
        }
    },
	"attributes": {
		"PluggableAuth": {
			"JWTAuth": {
				"class": "MediaWiki\\Extension\\JWTAuth\\JWTAuth",
				"services": [
					"MainConfig",
					"UserFactory"
				]
			}
		}
	},
    "load_composer_autoloader": true
}
